{
    "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
    "type": "datasource",
    "id": "fluvio-dynamodb-datasource",
    "name": "Fluvio DynamoDB",
    "backend": true,
    "executable": "fluvio-dynamodb-datasource",
    "metrics": true,
    "annotations": true,
    "alerting": true,
    "queryOptions": {
        "maxDataPoints": true,
        "minInterval": true
    },
    "info": {
        "author": {
            "name": "Fluvio"
        },
        "description": "AWS DynamoDB data source for Grafana",
        "keywords": [
            "dynamodb",
            "aws",
            "database",
            "nosql",
            "json"
        ],
        "logos": {
            "small": "img/logo.webp",
            "large": "img/logo.webp"
        },
        "version": "1.0.0",
        "updated": "2025-09-25"
    },
    "includes": [],
    "dependencies": {
        "grafanaDependency": ">=10.4.0",
        "plugins": []
    },
    "buildMode": "production"
}